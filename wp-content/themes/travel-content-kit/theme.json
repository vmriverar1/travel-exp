		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "Tiny",
					"size": "0.25rem",
					"slug": "20"
				},
				{
					"name": "X-Small",
					"size": "0.5rem",
					"slug": "30"
				},
				{
					"name": "Small",
					"size": "0.75rem",
					"slug": "40"
				},
				{
					"name": "Regular",
					"size": "1rem",
					"slug": "50"
				},
				{
					"name": "Medium",
					"size": "clamp(1.25rem, 1.1429rem + 0.4762vw, 1.5rem)",
					"slug": "60"
				},
				{
					"name": "Large",
					"size": "clamp(1.25rem, 1.0357rem + 0.9524vw, 1.75rem)",
					"slug": "70"
				},
