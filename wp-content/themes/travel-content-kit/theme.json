				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fluid": true,
			"defaultFontSizes": false,
			"fontFamilies": [
				{
					"fontFamily": "Satoshi, sans-serif",
					"name": "Satoshi",
					"slug": "satoshi",
					"fontFace": [
						{
							"fontFamily": "Satoshi",
							"fontWeight": "300 900",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/satoshi/Satoshi-Variable.woff2"
							]
						},
						{
							"fontFamily": "Satoshi",
							"fontWeight": "300 900",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/satoshi/Satoshi-VariableItalic.woff2"
							]
						}
					]
				},
