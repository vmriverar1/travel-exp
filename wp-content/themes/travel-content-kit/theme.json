					"name": "Regular",
					"size": "1rem",
					"slug": "regular"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.25rem"
					},
					"name": "Medium",
					"size": "1.25rem",
					"slug": "medium"
				},
